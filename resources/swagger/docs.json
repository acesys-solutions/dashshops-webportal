    "/register": {
      "post": {
        "summary": "Create new user",
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "firstname": {
                    "type": "string",
                    "example": "John"
                  },
                  "lastname": {
                    "type": "string",
                    "example": "Doe"
                  },
                  "email": {
                    "type": "string",
                    "example": "john@doe.com"
                  },
                  "phone_number": {
                    "type": "string",
                    "example": "+2348010203040"
                  },
                  "password": {
                    "type": "string",
                    "example": "password"
                  }
                },
                "required": [
                  "firstname",
                  "lastname",
                  "email",
                  "phone_number",
                  "password"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "User Created Successfully"
                    },
                    "token": {
                      "type": "string",
                      "example": "1234567890abcdef"
                    },
                    "data": {
                      "$ref": "#/components/schemas/User"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error500"
                }
              }
            }
          }
        }
      }
    },
    "/register/retailer": {
      "post": {
        "summary": "Create new retailer",
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "banner_image": {
                    "type": "file"
                  },
                  "business_name": {
                    "type": "string",
                    "example": "Example Corp"
                  },
                  "business_address": {
                    "type": "string",
                    "example": "123, Example Street, Example City"
                  },
                  "business_description": {
                    "type": "string",
                    "example": "Example Description"
                  },
                  "type_of_business": {
                    "type": "number",
                    "example": 7
                  },
                  "business_hours_open": {
                    "type": "string",
                    "example": "Monday-Friday:11am-11pm"
                  },
                  "business_hours_close": {
                    "type": "string",
                    "example": "Saturday-Sunday:9am-6pm"
                  },
                  "firstname": {
                    "type": "string",
                    "example": "John"
                  },
                  "lastname": {
                    "type": "string",
                    "example": "Doe"
                  },
                  "email": {
                    "type": "string",
                    "example": "john@doe.com"
                  },
                  "phone_number": {
                    "type": "string",
                    "example": "+2348010203040"
                  },
                  "city": {
                    "type": "string",
                    "example": "Example City"
                  },
                  "state": {
                    "type": "string",
                    "example": "Example State"
                  },
                  "zip_code": {
                    "type": "string",
                    "example": "12345"
                  },
                  "web_url": {
                    "type": "string",
                    "example": "https://example.com"
                  },
                  "latitude": {
                    "type": "number",
                    "example": 37.7749
                  },
                  "longitude": {
                    "type": "number",
                    "example": -122.4194
                  },
                  "password": {
                    "type": "string",
                    "example": "",
                    "description": "Not required if user with the email and phone exists"
                  }
                },
                "required": [
                  "banner_image",
                  "business_name",
                  "business_address",
                  "business_description",
                  "business_hours_open",
                  "business_hours_close",
                  "type_of_business",
                  "firstname",
                  "lastname",
                  "email",
                  "phone_number",
                  "city",
                  "state",
                  "zip_code",
                  "web_url",
                  "latitude",
                  "longitude"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "Retailer Created Successfully"
                    },
                    "token": {
                      "type": "string",
                      "example": "1234567890abcdef"
                    },
                    "data": {
                      "$ref": "#/components/schemas/Retailer"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error500"
                }
              }
            }
          }
        }
      }
    },
